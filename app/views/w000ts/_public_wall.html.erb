<div id="w000t-wall-container" class="grid">
  <% w000ts.each do |w000t| %>
    <figure class="item col-lg-3 col-md-4 col-xs-6">
      <%= link_to( image_tag(
        w000t.url_info.url,
        onerror: "this.style.display='none'",
        class:"center-block img-thumbnail img-responsive hidden"
      ), w000t_redirect_path(w000t.short_url) ) %>
      <div class="menu">
        <%= render partial: 'w000ts/image_w000t_share', locals: { w000t: w000t } %>
      </div>
    </figure>
  <% end %>
</div>

<%= paginate @w000ts %>
